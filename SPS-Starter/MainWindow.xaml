                                <Button x:Name="ButtonStartenTwinCatPlcDt" Content="{Binding ViAnzeige.StartButtonInhalt}" Grid.Column="3" Grid.Row="1" Background="{Binding ViAnzeige.StartButtonFarbe}" Command="{Binding BtnProjektStarten}" />
                                <WebBrowser x:Name="WebTwinCatPlcDt" Grid.Column="3" Grid.Row="3" />
                            </Grid>
                        </TabItem>

